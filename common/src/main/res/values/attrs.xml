<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="imageHeight" format="dimension"/>
    <attr name="imageWidth" format="dimension"/>
    <attr name="imageSrc" format="reference"/>
    <attr name="label" format="reference|string"/>
    <attr name="borderWidth" format="dimension" />
    <attr name="borderColor" format="reference|color" />
    <attr name="borderFocusColor" format="reference|color" />
    <attr name="enableSelected" format="boolean" />
    <attr name="enablePressed" format="boolean" />
    <attr name="expandedText" format="string" />
    <attr name="collapsedText" format="string" />
    <attr name="trimLines" format="integer" />
    <attr name="strokeColor" format="reference|color" />
    <attr name="strokeWidth" format="dimension" />
    <attr name="strokeHeight" format="dimension" />
    <attr name="radius" format="dimension" />
    <attr name="textColor" format="color" />
    <attr name="textSize" format="dimension" />
    <attr name="bgColor" format="reference|color" />
    <attr name="text" format="string" />
    <attr name="leftTopRadius" format="dimension" />
    <attr name="rightTopRadius" format="dimension" />
    <attr name="rightBottomRadius" format="dimension" />
    <attr name="leftBottomRadius" format="dimension" />

    <declare-styleable name="CircleImageView">
        <attr name="borderWidth"/>
        <attr name="borderColor"/>
        <attr name="borderFocusColor"/>
        <attr name="enableSelected"/>
        <attr name="enablePressed"/>
    </declare-styleable>

    <declare-styleable name="scaling_text_view">
        <attr name="text"/>
        <attr name="defaultLine" format="integer"/>
        <attr name="textColor"/>
        <attr name="defaultCollapsed" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="RatingBar">
        <!--星星间距-->
        <attr name="starDistance" format="dimension" />
        <!--星星大小-->
        <attr name="starSize" format="dimension" />
        <!--星星个数-->
        <attr name="starCount" format="integer" />
        <!--星星空图-->
        <attr name="starEmpty" format="reference" />
        <!--星星满图-->
        <attr name="starFill" format="reference" />
    </declare-styleable>
    <!--自动换行-->
    <declare-styleable name="WarpLinearLayout">
        <attr name="gravity" format="enum"><!--对齐方式 !-->
            <enum name="right" value="0"/>
            <enum name="left" value="1"/>
            <enum name="center" value="2"/>
        </attr>
        <attr name="horizontalSpace" format="dimension"/>
        <attr name="verticalSpace" format="dimension"/>
        <attr name="isFull" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="auto3d">
        <attr name="textSize"/>
        <attr name="textColor"/>
        <attr name="text" />
        <attr name="bgSelected" format="boolean" />
    </declare-styleable>

    <declare-styleable name="IconDotTextView">
        <attr name="textSize" />
        <attr name="textColor" />
        <attr name="imageHeight" />
        <attr name="imageWidth" />
        <attr name="imageSrc"/>
        <attr name="label"/>
    </declare-styleable>
    <declare-styleable name="CircleTextView">
        <attr name="strokeColor" />
        <attr name="bgColor"/>
        <attr name="strokeWidth"/>
    </declare-styleable>

    <declare-styleable name="Custom_Progress_Bar">
        <attr name="bgColor"/>
        <attr name="textColor" />
        <attr name="progressColor" format="reference|color" />
        <attr name="strokeWidth"/>
        <attr name="showText" format="boolean" />
        <attr name="textSize"/>
        <attr name="fontPercent" format="integer" />
        <attr name="bgRadius" format="dimension" />
        <attr name="maxProgress" format="float" />
        <attr name="progress" format="float" />
    </declare-styleable>

    <declare-styleable name="MoreTextView">
        <attr name="expandedText" />
        <attr name="collapsedText" />
        <attr name="trimLines" />
        <attr name="textSize" />
        <attr name="textColor" />
    </declare-styleable>

    <declare-styleable name="CollapsedTextView">
        <attr name="expandedText" />
        <attr name="collapsedText" />
        <attr name="trimLines" />
    </declare-styleable>

    <declare-styleable name="BannerView">
        <!--是否可以点击浏览大图-->
        <attr name="bgColor"/>
        <!--是否可以点击浏览大图-->
        <attr name="previewLarger" format="boolean" />
        <!--自动轮播间隔时间，默认为3000毫秒，单位为毫秒-->
        <attr name="loopInterval" format="integer" />
        <!--是否自动轮播-->
        <attr name="autoLoop" format="boolean" />
        <!--针位置，是在banner内部底下还是外部底下，暂未实现这个功能，out属性用处不大感觉就没写了-->
        <attr name="dotPosition" format="enum">
            <enum name="inner_bottom_center" value="0" />
            <enum name="inner_bottom_left" value="1" />
            <enum name="inner_bottom_right" value="2" />
            <enum name="outer_bottom_center" value="3" />
            <enum name="outer_bottom_left" value="4" />
            <enum name="outer_bottom_right" value="5" />
        </attr>
        <!--指针总高度，主要是是在dot_position为outer时使用-->
        <attr name="dotHeight" format="dimension"/>
        <!--指针距离底部距离，并不是点之间的距离-->
        <attr name="dotBottomMargin" format="dimension"/>
        <!--指针距离左边距离，并不是点之间的距离-->
        <attr name="dotLeftMargin" format="dimension"/>
        <!--指针距离右边距离，并不是点之间的距离-->
        <attr name="dotRightMargin" format="dimension"/>
        <!--指针点之间的距离-->
        <attr name="dotPadding" format="dimension"/>
        <!--占位图-->
        <attr name="bannerPlaceholderImage" format="reference"/>
        <attr name="leftTopRadius"/>
        <attr name="rightTopRadius"/>
        <attr name="rightBottomRadius"/>
        <attr name="leftBottomRadius"/>
        <attr name="iconSelected" format="reference" />
        <attr name="iconUnselected" format="reference" />
    </declare-styleable>

    <declare-styleable name="CornerTextView">
        <attr name="strokeColor" />
        <attr name="bgColor"/>
        <attr name="strokeWidth"/>
        <attr name="radius"/>
    </declare-styleable>

    <declare-styleable name="Custom_Round_Image_View">
        <attr name="radius"/>
        <attr name="leftTopRadius"/>
        <attr name="rightTopRadius"/>
        <attr name="rightBottomRadius"/>
        <attr name="leftBottomRadius"/>
        <attr name="bgColor"/>
    </declare-styleable>

    <declare-styleable name="CornerLabelView">
        <attr name="sideLength" format="dimension" />
        <attr name="marginLeanSide" format="dimension" />
        <attr name="bgColor"/>
        <attr name="text"/>
        <attr name="textSize" />
        <attr name="textColor"/>
        <attr name="position">
            <enum name="rightTop" value="0" />
            <enum name="rightBottom" value="1" />
            <enum name="leftBottom" value="2" />
            <enum name="leftTop" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="verification_code">
        <!--验证码框大小-->
        <attr name="strokeWidth"/>
        <attr name="strokeHeight"/>
        <!--验证码两个框之间边距-->
        <attr name="codeStrokePadding" format="dimension" />
        <!--验证码框圆角-->
        <attr name="radius"/>
        <!--验证码边框粗细-->
        <attr name="codeStrokeBorderWidth" format="dimension" />
        <!--验证码框有焦点时边框颜色-->
        <attr name="codeStrokeColorStateFocusedTrue" format="color" />
        <!--验证码框无焦点时边框颜色-->
        <attr name="codeStrokeColorStateFocusedFalse" format="color" />
        <!--验证码框有焦点时背景色-->
        <attr name="codeBgColorStateFocusedTrue" format="color" />
        <!--验证码框无焦点时背景色-->
        <attr name="codeBgColorStateFocusedFalse" format="color" />
        <!--光标粗细-->
        <attr name="cursorWidth" format="dimension" />
        <!--光标高度-->
        <attr name="cursorHeight" format="dimension" />
        <!--有焦点时光标颜色-->
        <attr name="cursorBgColorStateFocusedTrue" format="color" />
        <!--无焦点时光标颜色-->
        <attr name="cursorBgColorStateFocusedFalse" format="color" />
        <!--验证码最大支持长度-->
        <attr name="codeMaxLength" format="integer" />
        <!--验证码输入类型，纯数字、纯字母、数字+字母-->
        <attr name="codeType">
            <enum name="number" value="0" />
            <enum name="character" value="1" />
            <enum name="all" value="2" />
        </attr>

    </declare-styleable>
    <declare-styleable name="CustomEditText">
        <attr name="drawablePosition">
            <enum name="left" value="1" />
            <enum name="top" value="2" />
            <enum name="right" value="3" />
            <enum name="bottom" value="4" />
        </attr>
        <attr name="searchIcon" format="reference" />
        <attr name="clearIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="MaterialRipple">
        <attr name="viewItemBackground" format="reference" />
        <attr name="viewItemForeground" format="reference" />
        <attr name="viewItemForegroundBorderless" format="reference" />
    </declare-styleable>
    <declare-styleable name="GradationRectTextView">
        <attr name="shapeType" format="integer|enum">
            <enum name="rectangle" value="0" />
            <enum name="oval" value="1" />
            <enum name="LINE" value="2" />
        </attr>
        <attr name="totalRadius" format="dimension" />
        <attr name="radiusTopLeft" format="dimension" />
        <attr name="radiusBottomLeft" format="dimension" />
        <attr name="radiusTopRight" format="dimension" />
        <attr name="radiusBottomRight" format="dimension" />
        <attr name="topLeft" format="dimension" />
        <attr name="topRight" format="dimension" />
        <attr name="bottomLeft" format="dimension" />
        <attr name="bottomRight" format="dimension" />
        <attr name="strColor" format="color" />
        <attr name="strWidth" format="dimension" />
        <attr name="solidBac" format="color" />
        <attr name="textPadding" format="dimension" />
        <attr name="textLeft" format="string" />
        <attr name="textRight" format="string" />
        <attr name="iconColor" format="reference|color" />
        <attr name="textLeftColor" format="reference|color" />
        <attr name="textRightColor" format="reference|color" />
        <attr name="textLeftSize" format="dimension" />
        <attr name="textRightSize" format="dimension" />
        <attr name="textLeftStyle">
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
        </attr>
        <attr name="textRightStyle">
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
        </attr>
        <attr name="textCenterStyle">
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
        </attr>
        <attr name="autoMaxHeight" format="boolean" />
        <attr name="gradientOrientation">
            <enum name="top_bottom" value="0" />
            <enum name="tp_bl" value="1" />
            <enum name="right_left" value="2" />
            <enum name="br_tl" value="3" />
            <enum name="bottom_top" value="4" />
            <enum name="bl_tr" value="5" />
            <enum name="left_right" value="6" />
            <enum name="tl_br" value="7" />
        </attr>
        <attr name="startSolid" format="reference|color" />
        <attr name="centerSolid" format="reference|color" />
        <attr name="endSolid" format="reference|color" />
        <!--        <attr name="fang_zheng_regular_font" format="boolean" />-->
        <!--        <attr name="fang_zheng_bold_font" format="boolean" />-->
    </declare-styleable>
    <declare-styleable name="CustomViewfinderView">
        <attr name="lineColor" format="color" /><!--扫描线的颜色-->
        <attr name="cornerColor" format="color" /><!--四边角的颜色-->
        <attr name="scanFrameWidth" format="dimension" /><!--扫描框的宽度-->
        <attr name="scanFrameHeight" format="dimension" /><!--扫描框的高度-->
    </declare-styleable>

    <!-- 垂直方向的虚线 -->
    <declare-styleable name="DividerView">
        <!-- 虚线颜色 -->
        <attr name="dividerLineColor" format="color"/>
        <!-- 虚线宽度 -->
        <attr name="dashThickness" format="dimension"/>
        <!-- 虚线dash宽度 -->
        <attr name="dashLength" format="dimension"/>
        <!-- 虚线dash间隔 -->
        <attr name="dashGap" format="dimension"/>
        <!-- 虚线朝向 -->
        <attr name="dividerOrientation" format="enum">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
    </declare-styleable>
</resources>