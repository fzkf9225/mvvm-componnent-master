<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        xmlns:tick_view="http://schemas.android.com/apk/res-auto"
        android:background="@drawable/rounded_white_16"
        android:paddingStart="@dimen/horizontal_margin_xl"
        android:paddingEnd="@dimen/horizontal_margin_xl"
        android:paddingTop="@dimen/padding_xl"
        android:paddingBottom="@dimen/padding_xl"
        android:orientation="vertical">

        <pers.fz.mvvm.wight.tickview.TickView
                android:id="@+id/tick_view"
                android:layout_width="@dimen/x150"
                android:layout_height="@dimen/x150"
                android:layout_gravity="center"
                tick_view:clickable="false"
                tick_view:rate="normal" />
        <TextView
                android:id="@+id/dialog_textView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/padding_xl"
                android:textColor="@color/autoColor"
                android:gravity="center"
                android:textSize="@dimen/font_size_xl"
                tools:text="设置成功" />

</LinearLayout>
